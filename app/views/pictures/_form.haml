= form_for [@album, @picture], html_options:{multipart:true} do |f|
  - if @picture.errors.any?
    #error_explanation
      %h2= "#{pluralize(@picture.errors.count, "error")} prohibited this abc from being saved:"
      %ul
        - @picture.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :url
    = f.file_field :url
  .actions
    = f.submit 'Save'
