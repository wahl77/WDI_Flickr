%h1 Sessions#new
%p Find me in app/views/sessions/new.html.haml

= form_tag sessions_path do 
  .field
    = label_tag :username
    = text_field_tag :username, params[:username]

  .field
    = label_tag :email
    = text_field_tag :email, params[:email]

  .field
    = label_tag :password
    = password_field_tag :password

  .actions
    = submit_tag "Submit"
